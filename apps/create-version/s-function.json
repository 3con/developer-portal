{
  "name": "apps-create-version",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: keboola-developer-portal",
  "customName": false,
  "customRole": false,
  "handler": "create-version/handler.handler",
  "timeout": 30,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "apps/{appId}/versions",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "custom",
      "authorizerFunction": "authorizer",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": "{\"token\":\"$input.params().header.get('Authorization')\", \"body\":$input.json('$')}"
      },
      "responses": "$${endpointResponsesTemplate}"
    }
  ],
  "events": [],
  "environment": {
    "REGION": "${region}",
    "RDS_HOST": "${rdsHost}",
    "RDS_USER": "${rdsUser}",
    "RDS_PASSWORD": "${rdsPassword}",
    "RDS_DATABASE": "${rdsDatabase}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}