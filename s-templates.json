{
  "endpointResponsesTemplate": {
    "400": {
      "statusCode": "400"
    },
    "401": {
      "statusCode": "401"
    },
    "404": {
      "statusCode": "404"
    },
    "409": {
      "statusCode": "409"
    },
    "null": {
      "statusCode": "204",
      "responseModels": {
        "application/json": "Empty"
      },
      "responseTemplates": {
        "application/json": ""
      }
    },
    "default": {
      "statusCode": "200",
      "responseParameters": {},
      "responseModels": {
        "application/json": "Empty"
      },
      "responseTemplates": {
        "application/json": ""
      }
    }
  },
  "endpointResponseNoContent": {
    "400": {
      "statusCode": "400"
    },
    "401": {
      "statusCode": "401"
    },
    "404": {
      "statusCode": "404"
    },
    "409": {
      "statusCode": "409"
    },
    "default": {
      "statusCode": "204",
      "responseParameters": {},
      "responseModels": {
        "application/json": "Empty"
      },
      "responseTemplates": {
        "application/json": ""
      }
    }
  },
  "responsesDefault": {
    ".*400.*": "$${response400}",
    ".*Task timed out.*": "$${response504}",
    ".*\"errorMessage\".*": "$${response500}",
    "default": "$${response200}"
  },
  "responsesForPost": {
    ".*400.*": "$${response400}",
    ".*Task timed out.*": "$${response504}",
    ".*(\"errorMessage\")|(Process exited).*": "$${response500}",
    "default": "$${response201}"
  },
  "response200": {
    "statusCode": "200",
    "responseParameters": "$${responseParameters}",
    "responseTemplates": {
      "application/json": ""
    }
  },
  "response201": {
    "statusCode": "201",
    "responseParameters": "$${responseParameters}",
    "responseTemplates": {
      "application/json": ""
    }
  },
  "response204": {
    "statusCode": "204",
    "responseParameters": "$${responseParameters}",
    "responseModels": {
      "application/json": "Empty"
    },
    "responseTemplates": {
      "application/json": ""
    }
  },
  "response400": {
    "statusCode": "400",
    "responseModels": {},
    "responseTemplates": {
      "application/json": "{\"message\": \"$input.path('$.errorMessage')\",\"code\": \"$input.path('$.errorType')\"}"
    }
  },
  "response500": {
    "statusCode": "500",
    "responseTemplates": {
      "application/json": "{\"message\": \"Server Error\"}"
    }
  },
  "response504": {
    "statusCode": "504",
    "responseTemplates": {
      "application/json": "{\"message\": \"Gateway Timeout\"}"
    }
  },
  "responseParameters": {
    "method.response.header.location": "integration.response.body.links.self"
  }
}